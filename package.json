{
  "name": "tag-manager",
  "displayName": "tag-manager",
  "description": "tag-manager",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tag-manager.searchPanelTag",
    "onCommand:tag-manager.searchInFiles",
    "onCommand:tag-manager.searchInFile",
    "onCommand:tag-manager.replaceInFiles",
    "onCommand:tag-manager.replaceInFile",
    "onCommand:tag-manager.revertChanges"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tag-manager.searchPanelTag",
        "title": "Structural Search and Replace Panel"
      },
      {
        "command": "tag-manager.searchInFiles",
        "title": "Structural Search in All Files"
      },
      {
        "command": "tag-manager.searchInFile",
        "title": "Structural Search in Current File"
      },
      {
        "command": "tag-manager.replaceInFiles",
        "title": "Structural Replace in All Files"
      },
      {
        "command": "tag-manager.replaceInFile",
        "title": "Structural Replace in Current File"
      },
      {
        "command": "tag-manager.revertChanges",
        "title": "Revert Changes"
      }
    ],
    "keybindings": [
      {
        "command": "tag-manager.searchPanelTag",
        "key": "ctrl+1"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/shelljs": "^0.8.11",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "jsdom": "^20.0.0",
    "pretty": "^2.0.0",
    "selector-2-regexp": "^1.0.0",
    "shelljs": "^0.8.5"
  }
}
